{
  "name": "carbook",
  "version": "1.0.0-alpha",
  "description": "CarBook",
  "main": "index.js",
  "engines": {
    "node": ">=4.5.0",
    "npm": "3.0.0"
  },
  "scripts": {
    "clean": "rimraf dist",
    "compile": "better-npm-run compile",
    "lint": "eslint bin build config server src tests",
    "lint:fix": "npm run lint -- --fix",
    "start": "better-npm-run start",
    "debug": "better-npm-run debug",
    "dev": "better-npm-run dev",
    "test": "mocha --compilers js:babel-core/register --require ./tests/test_helper.js --require ignore-styles --recursive ./tests",
    "test:watch": "npm run test -- --watch",
    "test:dev": "npm run test -- --watch",
    "deploy": "better-npm-run deploy",
    "devbuild": "better-npm-run deploy:dev",
    "qabuild": "better-npm-run deploy:qa",
    "prodbuild": "better-npm-run deploy:prod",
    "codecov": "cat coverage/*/lcov.info | codecov"
  },
  "betterScripts": {
    "compile": {
      "command": "node bin/compile",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "dev": {
      "command": "nodemon bin/dev-server --ignore dist --ignore coverage --ignore tests --ignore src",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "deploy": {
      "command": "npm run clean && npm run compile",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "deploy:dev": {
      "command": "npm run deploy",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "deploy:qa": {
      "command": "npm run deploy",
      "env": {
        "NODE_ENV": "qa",
        "DEBUG": "app:*"
      }
    },
    "deploy:prod": {
      "command": "npm run deploy",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    },
    "start": {
      "command": "npm run lint:fix && node bin/dev-server",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "test": {
      "command": "node ./node_modules/karma/bin/karma start config/karma.config",
      "env": {
        "NODE_ENV": "test",
        "DEBUG": "app:*"
      }
    },
    "debug": {
      "command": "npm run lint:fix && node --debug-brk bin/dev-server",
      "env": {
        "DEBUG": "app:*"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dcoric/react-redux-starter-kit.git"
  },
  "author": "Anonymous",
  "license": "MIT",
  "dependencies": {
    "axios": "0.17.1",
    "babel-core": "6.26.0",
    "babel-loader": "7.1.2",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-1": "6.24.1",
    "better-npm-run": "0.1.0",
    "bootstrap": "4.0.0-alpha.6",
    "compression": "1.7.1",
    "css-loader": "0.28.7",
    "cssnano": "3.10.0",
    "debug": "3.1.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.5",
    "font-awesome": "4.7.0",
    "fs-extra": "4.0.2",
    "graceful-fs": "4.1.15",
    "history": "4.7.2",
    "html-webpack-plugin": "2.30.1",
    "immutable": "3.8.2",
    "ip": "1.1.5",
    "json-loader": "0.5.7",
    "lodash": "4.17.4",
    "material-ui-upload": "1.2.1",
    "moment": "2.19.2",
    "moment-timezone": "0.5.16",
    "node-sass": "4.9.2",
    "normalize.css": "7.0.0",
    "postcss-loader": "2.0.9",
    "react": "16.1.1",
    "react-bootstrap": "0.31.5",
    "react-bootstrap-typeahead": "3.0.1",
    "react-chat-elements": "0.8.1",
    "react-datetime": "2.11.0",
    "react-dom": "16.1.1",
    "react-dropzone-component": "3.0.0",
    "react-images-upload": "1.1.1",
    "react-redux": "5.0.6",
    "react-router": "4.2.0",
    "react-router-dom": "4.2.2",
    "react-tap-event-plugin": "3.0.2",
    "react-widgets": "4.1.1",
    "redux": "3.7.2",
    "redux-form": "7.1.2",
    "redux-thunk": "2.2.0",
    "rimraf": "2.6.2",
    "sass-loader": "6.0.6",
    "style-loader": "0.19.0",
    "url-loader": "0.6.2",
    "webpack": "3.8.1",
    "yargs": "10.0.3",
    "zip-webpack-plugin": "2.1.0"
  },
  "devDependencies": {
    "babel-eslint": "8.0.2",
    "chai": "4.1.2",
    "chai-jquery": "2.0.0",
    "eslint": "4.11.0",
    "eslint-config-standard": "10.2.1",
    "eslint-config-standard-react": "5.0.0",
    "eslint-plugin-babel": "4.1.2",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-node": "5.2.1",
    "eslint-plugin-promise": "3.6.0",
    "eslint-plugin-react": "7.5.1",
    "eslint-plugin-standard": "3.0.1",
    "express": "4.16.2",
    "jquery": "3.2.1",
    "jsdom": "11.4.0",
    "mocha": "4.0.1",
    "react-addons-test-utils": "15.6.2",
    "redbox-react": "1.5.0",
    "webpack-dev-middleware": "1.12.1",
    "webpack-hot-middleware": "2.20.0"
  }
}
